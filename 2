import React from 'react';

const VideoLibrary = () => {
  const videos = [
    {
      title: 'Athlete Name - Workout Title',
      ipfsHash: 'QmXtvPwB1eCkF6gDt2wv7LJLcvLLJET3ZQbVr1MqUwRDJX',
    },
    {
      title: 'Athlete Name - Workout Title',
      ipfsHash: 'QmXtvPwB1eCkF6gDt2wv7LJLcvLLJET3ZQbVr1MqUwRDJX',
    },
    // Add more video objects as needed
  ];

  const renderVideos = () => {
    return videos.map((video, index) => (
      <div className="video-container" key={index}>
        <h2>{video.title}</h2>
        <video controls>
          <source
            src={`https://ipfs.io/ipfs/${video.ipfsHash}`}
            type="video/mp4"
          />
        </video>
      </div>
    ));
  };

  return (
    <div>
      <h1>Fitness Video Library</h1>
      {renderVideos()}
    </div>
  );
};

export default VideoLibrary;

